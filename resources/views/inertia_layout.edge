      }

      #loader-text {
        font-size: 2.5rem;
        color: #ffffff;
        letter-spacing: 0.05em;
        min-width: 200px;
        display: inline-block;
        transition: font-family 0.2s ease;
      }

      #loader-text::after {
        content: '|';
        animation: blink 1s step-end infinite;
        margin-left: 2px;
      }

      @keyframes blink {
        0%, 50% {
          opacity: 1;
        }

        51%, 100% {
          opacity: 0;
        }
      }
    </style>

    @inertia()
  </body>

</html>

